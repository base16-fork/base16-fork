<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Ayu Dark">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Ayu Dark scheme by Khue Nguyen <Z5483Y@gmail.com>
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#E6E1CF" background="#0F1419"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#272D38"/>
  <style name="CurrentLine" background="#3E4B59"/>
  <style name="LineNumber" foreground="#3E4B59"/>
  <style name="CurrentLineNumber" foreground="#BFBDB6" bold="true"/>
  <style name="SearchResult" background="#131721"/>
  <style name="Parentheses" background="#272D38" />
  <style name="ParenthesesMismatch" background="#D2A6FF" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#F3F4F5" background="#3E4B59"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#3E4B59"/>
  <style name="Occurrences.Rename" background="#BFBDB6"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#FF8F40"/>

  <!-- language primitives -->
  <style name="Number" foreground="#FF8F40"/>
  <style name="String" foreground="#B8CC52"/>
  <style name="Keyword" foreground="#D2A6FF"/>
  <style name="Operator" foreground="#E6E1CF"/>
  <style name="Preprocessor" foreground="#59C2FF"/>
  <style name="Label" foreground="#F07178"/>

  <!-- types, classes -->
  <style name="Type" foreground="#FFB454"/>
  <style name="PrimitiveType" foreground="#D2A6FF"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#59C2FF"/>
  <style name="VirtualMethod" foreground="#59C2FF" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#E6E1CF"/>
  <style name="Local" foreground="#E6E1CF"/>
  <style name="Global" foreground="#E6E1CF"/>
  <style name="Static" foreground="#E6E1CF"/>
  <style name="JsScopeVar" foreground="#E6E1CF" italic="true"/>
  <style name="JsImportVar" foreground="#E6E1CF" italic="true"/>
  <style name="JsGlobalVar" foreground="#E6E1CF" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#272D38"/>

  <!-- comments -->
  <style name="Comment" foreground="#3E4B59"/>
  <style name="DisabledCode" foreground="#3E4B59"/>
  <style name="Doxygen.Comment" foreground="#3E4B59"/>
  <style name="Doxygen.Tag" foreground="#3E4B59"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#D2A6FF"/>
  <style name="Binding" foreground="#F07178"/>
  <style name="QmlExternalId" foreground="#59C2FF" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#59C2FF" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#D2A6FF"/>
  <style name="DiffLocation" foreground="#59C2FF"/>
  <style name="AddedLine" foreground="#B8CC52"/>
  <style name="RemovedLine" foreground="#FFB454"/>
  <style name="LogChangeLine" foreground="#F07178"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#D2A6FF"/>
  <style name="DiffContextLine" foreground="#59C2FF"/>
  <style name="DiffSourceLine" foreground="#B8CC52"/>
  <style name="DiffSourceChar" foreground="#FFB454" background="#131721"/>
  <style name="DiffDestLine" foreground="#B8CC52"/>
  <style name="DiffDestChar" foreground="#FFB454" background="#131721"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#F07178" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#F07178" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#FFB454" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#FFB454" underlineStyle="DotLine"/>
</style-scheme>
