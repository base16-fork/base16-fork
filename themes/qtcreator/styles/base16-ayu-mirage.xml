<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Ayu Mirage">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Ayu Mirage scheme by Khue Nguyen <Z5483Y@gmail.com>
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#CCCAC2" background="#171B24"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#242936"/>
  <style name="CurrentLine" background="#707A8C"/>
  <style name="LineNumber" foreground="#707A8C"/>
  <style name="CurrentLineNumber" foreground="#8A9199" bold="true"/>
  <style name="SearchResult" background="#1F2430"/>
  <style name="Parentheses" background="#242936" />
  <style name="ParenthesesMismatch" background="#D4BFFF" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#F3F4F5" background="#707A8C"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#707A8C"/>
  <style name="Occurrences.Rename" background="#8A9199"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#FFAD66"/>

  <!-- language primitives -->
  <style name="Number" foreground="#FFAD66"/>
  <style name="String" foreground="#D5FF80"/>
  <style name="Keyword" foreground="#D4BFFF"/>
  <style name="Operator" foreground="#CCCAC2"/>
  <style name="Preprocessor" foreground="#5CCFE6"/>
  <style name="Label" foreground="#F28779"/>

  <!-- types, classes -->
  <style name="Type" foreground="#FFD173"/>
  <style name="PrimitiveType" foreground="#D4BFFF"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#5CCFE6"/>
  <style name="VirtualMethod" foreground="#5CCFE6" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#D9D7CE"/>
  <style name="Local" foreground="#CCCAC2"/>
  <style name="Global" foreground="#CCCAC2"/>
  <style name="Static" foreground="#CCCAC2"/>
  <style name="JsScopeVar" foreground="#CCCAC2" italic="true"/>
  <style name="JsImportVar" foreground="#CCCAC2" italic="true"/>
  <style name="JsGlobalVar" foreground="#CCCAC2" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#242936"/>

  <!-- comments -->
  <style name="Comment" foreground="#707A8C"/>
  <style name="DisabledCode" foreground="#707A8C"/>
  <style name="Doxygen.Comment" foreground="#707A8C"/>
  <style name="Doxygen.Tag" foreground="#707A8C"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#D4BFFF"/>
  <style name="Binding" foreground="#F28779"/>
  <style name="QmlExternalId" foreground="#5CCFE6" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#5CCFE6" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#D4BFFF"/>
  <style name="DiffLocation" foreground="#5CCFE6"/>
  <style name="AddedLine" foreground="#D5FF80"/>
  <style name="RemovedLine" foreground="#FFD173"/>
  <style name="LogChangeLine" foreground="#F28779"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#D4BFFF"/>
  <style name="DiffContextLine" foreground="#5CCFE6"/>
  <style name="DiffSourceLine" foreground="#D5FF80"/>
  <style name="DiffSourceChar" foreground="#FFD173" background="#1F2430"/>
  <style name="DiffDestLine" foreground="#D5FF80"/>
  <style name="DiffDestChar" foreground="#FFD173" background="#1F2430"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#F28779" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#F28779" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#FFD173" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#FFD173" underlineStyle="DotLine"/>
</style-scheme>
